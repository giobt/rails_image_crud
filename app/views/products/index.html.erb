<h1>Listing products</h1>

<%= link_to 'New product', new_product_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.text %></td>
      <!-- <td><%= product.url %></td> -->
      <td><%= link_to 'Show', product_path(product) %></td>
    </tr>
  <% end %>
</table>
